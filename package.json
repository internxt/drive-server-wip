    "@nestjs/core": "^11.1.6",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/sequelize": "^11.0.0",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/throttler": "^6.4.0",
    "@sendgrid/mail": "^8.1.5",
    "@sentry/node": "^7.80.1",
    "@sentry/tracing": "^7.80.1",
    "@types/passport-jwt": "^3.0.13",
    "agentkeepalive": "^4.5.0",
    "axios": "^1.12.0",
    "bcryptjs": "^2.4.3",
    "cache-manager": "^6.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "geoip-lite": "^1.4.8",
    "hash-wasm": "^4.12.0",
    "helmet": "^7.2.0",
    "multer-s3": "^3.0.1",
    "nestjs-pino": "^4.4.0",
    "newrelic": "^11.23.2",
    "node-device-detector": "2.0.17",
    "openpgp": "^5.11.3",
    "passport": "^0.7.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "prettysize": "^2.0.0",
    "prom-client": "^15.0.0",
    "prometheus-api-metrics": "^4.0.0",
    "qrcode": "^1.4.4",
    "redis": "^5.8.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.2",
    "semver": "^7.7.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "speakeasy": "^2.0.0",
    "sql-formatter": "^13.1.0",
    "stripe": "14.4.0",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.6.2",
    "@internxt/eslint-config-internxt": "^1.0.9",
    "@nestjs/schematics": "^11.0.7",
    "@nestjs/testing": "^11.1.6",
    "@types/chance": "^1.1.6",
    "@types/crypto-js": "^4.2.1",
    "@types/express": "^5.0.1",
    "@types/jest": "29.5.14",
    "@types/jsonwebtoken": "9.0.2",
    "@types/multer": "^1.4.13",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^20.19.11",
    "@types/sequelize": "^4.28.20",
    "@types/speakeasy": "^2.0.6",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "aws-sdk-client-mock": "^4.0.0",
    "chance": "^1.1.13",
    "cross-env": "^7.0.3",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "29.7.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.5.3",
    "sequelize-cli": "^6.6.3",
    "source-map-support": "^0.5.21",
    "supertest": "^7.1.4",
    "ts-jest": "29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "roots": [
      "src",
      "test"
    ],
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ]
  },
  "lint-staged": {
    "./src/**/*.{js,ts}": "yarn run lint"
  }
}
