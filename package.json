    "helmet": "^7.1.0",
    "mariadb": "^3.2.2",
    "multer-s3": "^3.0.1",
    "nest-winston": "^1.9.4",
    "newrelic": "^11.23.1",
    "node-device-detector": "2.0.17",
    "openpgp": "^5.11.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.11.3",
    "prettysize": "^2.0.0",
    "prom-client": "^15.0.0",
    "prometheus-api-metrics": "^3.2.2",
    "qrcode": "^1.4.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.5",
    "rxjs": "^7.8.1",
    "sequelize": "^6.35.0",
    "sequelize-typescript": "^2.1.5",
    "speakeasy": "^2.0.0",
    "sql-formatter": "^13.1.0",
    "stripe": "14.4.0",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
