{
  "name": "boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "Internxt",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "NODE_ENV=production nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "NODE_ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "test:e2e:watch": "NODE_ENV=test jest --config ./test/jest-e2e.json --watchAll",
    "migrate": "NODE_ENV=development sequelize db:migrate",
    "migrate:undo": "NODE_ENV=development sequelize db:migrate:undo",
    "migrate:prod": "NODE_ENV=production sequelize db:migrate",
    "migrate:prod:undo": "NODE_ENV=production sequelize db:migrate:undo",
    "migrate:test": "NODE_ENV=test sequelize db:migrate",
    "migrate:test:undo": "NODE_ENV=test sequelize db:migrate:undo"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@internxt/inxt-js": "^2.0.3",
    "@internxt/lib": "^1.2.0",
    "@nestjs/axios": "^0.0.8",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.1",
    "@nestjs/core": "^8.0.0",
    "@nestjs/event-emitter": "^1.1.1",
    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/sequelize": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@ntegral/nestjs-sentry": "^3.0.7",
    "@sendgrid/mail": "^7.7.0",
    "@sentry/node": "^7.7.0",
    "@sentry/tracing": "^7.7.0",
    "@types/passport-jwt": "^3.0.6",
    "analytics-node": "^6.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "crypto": "^1.0.1",
    "crypto-js": "^4.1.1",
    "geoip-lite": "^1.4.6",
    "helmet": "^5.1.0",
    "mariadb": "^3.0.0",
    "nest-winston": "^1.6.2",
    "node-device-detector": "2.0.3",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.8.0",
    "prettysize": "^2.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sequelize": "^6.20.1",
    "sequelize-typescript": "^2.1.3",
    "sql-formatter": "^6.1.2",
    "stripe": "8.178.0",
    "swagger-ui-express": "^4.4.0",
    "uuid": "^8.3.2",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@jbiskur/nestjs-test-utilities": "^3.4.1",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.8",
    "@types/jest": "27.5.0",
    "@types/node": "^17.0.38",
    "@types/sequelize": "^4.28.13",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "sequelize-cli": "^6.4.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
